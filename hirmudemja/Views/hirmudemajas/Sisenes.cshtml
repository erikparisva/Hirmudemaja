@model IEnumerable<hirmudemja.Models.SisenenudLahkunud>

@{
    ViewBag.Title = "Sisenes";
}

<h2>Sisenes</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Eesnimi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sisenes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Eesnimi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sisenes)
        </td>
        <td>
            @Html.ActionLink("Märgi Sisenenuks", "MargiSisenenuks", new { id = item.id }, new {@class = "my-class" }) |
            @Html.ActionLink("Pilet Tühistati", "PiletiTuhistamine", new { id = item.id }, new {@class = "my-class" })
        </td>
    </tr>
}

</table>
